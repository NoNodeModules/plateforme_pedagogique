{% extends 'base.html.twig' %}

{% block title %}Editer Question{% endblock %}

{% block logo %}
    <a class="navbar-brand ml-4" href="{{ path('teacher_index') }}">
        <img src="{{ asset('images/logo-carpa.svg') }}" class="logo-carpa" alt="logo ubuntu">
    </a>
{% endblock %}

{% block home %}
    <li class="nav-item">
        <a class="nav-link" href="{{ path('teacher_index') }}">Home</a>
    </li>
{% endblock %}

{% block profile %}
    <li class="nav-item">
        <a class="nav-link mr-2" href="{#{{ path('teacher_profile') }}#}">
            <img class="profile" src="{{ asset('images/profile.png') }}" alt="profile icon"></a>
    </li>
{% endblock %}

{% block body %}
    <h1>Editer Question</h1>
    {{ include('question/_form.html.twig') }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}

{% endblock %}